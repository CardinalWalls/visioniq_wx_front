@font-face {
  font-family: "fontNumBold";
  src: url('https://vision-pub.oss-cn-zhangjiakou.aliyuncs.com/DINPro-Bold.ttf');
}

@import "wxss/weui.wxss";
@import "wxss/middle.wxss";
@import "wxss/nosize.wxss";
@import "wxss/small.wxss";
@import "wxss/user.wxss";

page {
  background: #fff;
}

/* 改变富文本编辑器placeholder字体倾斜 */
.ql-editor.ql-blank:before {
  font-style: normal;
}

button {
  border: none;
  padding: 0 !important;

}

button::after {
  border: none;
}


/* 清除浮动*/

.fix {
  zoom: 1;
}

.fix:after {
  display: table;
  content: '';
  clear: both;
}


.block {
  display: block;
}

.navigator-hover {
  background-color: #fff;
  opacity: 1;
}

.pab {
  position: absolute;
}

.pre {
  position: relative;
}

/* 不定高宽元素水平垂直居中 */

.centrer1 {
  position: absolute;
  bottom: 0;
  right: 0;
  left: 0;
  top: 0;
  margin: auto;
}

.center2 {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.center4 {
  position: fixed;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.center3 {
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-align-items: center;
  align-items: center;
}



.fl {
  float: left;
}

.fr {
  float: right;
}

.db {
  display: block;
}

.dib {
  display: inline-block;
}



/* 横向flex */

.flex {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}

/* 兼容iPhoneX */

page {
  line-height: 1.3;
}

.ios {
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}


.dn {
  display: none;
}

.fe {
  -webkit-flex: 1;
  flex: 1;
}


.tl {
  text-align: left;
}

.tc {
  text-align: center;
}

.tr {
  text-align: right;
}

.btn-large {
  color: #fff;
  font-size: 38rpx;
  line-height: 96rpx;
  height: 96rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: bold;
  background-color: #01c176;
  border: 2rpx solid #01c176;
  border-radius: 96rpx;
}

.btn-large.gray {
  background-color: #a3a3a3;
  border-color: #a3a3a3;
}

.btn-large-border {
  background-color: #fff;
  color: #01c176;
  font-size: 38rpx;
  line-height: 96rpx;
  height: 96rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 2rpx solid #01c176;
  font-weight: bold;
  border-radius: 96rpx;
}

.input-placeholder {
  font-size: 30rpx;
  color: #a0a0a0;
}

.imgbg {
  background-color: #f5f5f5;
}

.bdtop {
  border-top: 10rpx solid #ececec;
}


.pd20 {
  padding: 0 20rpx;
}


/* fixed 按钮 */
.fixed-h {
  height: 142rpx;
}

.fixed-box {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  z-index: 100;
}

.fixed-box .fixed-pd {
  padding: 20rpx;
}

.container {
  position: relative;
  z-index: 1;
}

.opa {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}

/* 实名认证 */
.dialog-form {
  text-align: left;
  padding-bottom: 40rpx;
}

.dialog-form .input-box {
  background-color: #eeeeee;
  height: 88rpx;
  border-radius: 10rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.dialog-form .input-box:last-child {
  margin-bottom: 0;
}

.dialog-form .input-text {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 30rpx;
  color: #333333;
}

.dialog-form .input-placeholder {
  font-size: 30rpx;
  color: #868686;
}

/* 在线预约 */
.dialog-online {
  text-align: left;
  padding-bottom: 40rpx;
  margin-top: 30rpx;
}

.dialog-online .books {
  background-color: #fef0f0;
  color: #f56c6c;
  font-size: 26rpx;
  display: flex;
  padding: 10rpx;
  margin-bottom: 20rpx;
}

.dialog-online .books .tit {
  line-height: 48rpx;
  font-weight: bold;
}

.dialog-online .books .text-box {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
}

.dialog-online .books .text-box .time {
  margin-right: 10rpx;
  color: #fef0f0;
  background: #f56c6c;
  border: 1rpx solid #fbc4c4;
  font-size: 24rpx;
  padding: 0 10rpx;
  height: 48rpx;
  border-radius: 6rpx;
  position: relative;
  z-index: 1;
  display: flex;
  align-items: center;
}

.dialog-online .books .text-box .icon {
  margin-left: 4rpx;
}

.dialog-online .item {
  display: flex;
  font-size: 30rpx;
  color: #4a4a4a;
  height: 60rpx;
  align-items: center;
}


.dialog-online .item .tit {
