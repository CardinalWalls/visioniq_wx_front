/* components/nav-tab/nav-tab.wxss */
.nav-box {
  width: 100%;
}

button {
  border: none;
  padding: 0 !important;

}

button::after {
  border: none;
}
.nav-bar {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 99999999;
  -webkit-transition: background-color 0.5s;
  transition: background-color 0.5s;
  background-color: #fff;
  /* box-shadow: -3px 0 20rpx -2px rgba(0, 0, 0, 0.1);
  -webkit-box-shadow: -3px 0 20rpx -2px rgba(0, 0, 0, 0.1);
  -moz-box-shadow: -3px 0 20rpx -2px rgba(0, 0, 0, 0.1); */
}
.nav-bar.nofixed {
  position: relative;
}
.logo{
  width: 272rpx;
  height: 60rpx;
}

.title {
  text-align: center;
  font-size: 36rpx;
  color: #1a1a1a;
  width: 40%;
  margin: 0 auto;
  height: 45px;
  line-height: 45px;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  line-clamp: 1;
  -webkit-box-orient: vertical;
  font-weight: bold;
  position: relative;
  z-index: 6;
}

.title.small {
  font-weight: normal;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.type .nav-bar{
  border-bottom: 1px solid #f2f2f2;
}

.pre {
  position: relative;
}
.logo-box{
  height: 45px;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
}

/* // 黑色图标 */
.prev {
  width: 30% !important;
  position: absolute;
  left: 0;
  background: none;
  font-size: 30rpx;
  color: #fff;
  height: 45px;
  line-height: 45px;
  padding-left: 50rpx;
  text-align: left;
  background: url('data:image/png;base64,') no-repeat left 26rpx center;
  background-size: 21rpx 33rpx;
}

.nav-box.type0,
.nav-box.type3,
.nav-box.type1,
.nav-box.type4{
  position: absolute;
}

.nav-box.type0 .nav-bar,
.nav-box.type3 .nav-bar {
  background-color: transparent;
  box-shadow: 0 0 0 transparent;
}


.nav-box.type0 .title,
.nav-box.type1 .title,
.nav-box.type6 .title{
  color: #fff;
}


/* // 白色图标 */
.nav-box.type0 .prev,
.nav-box.type1 .prev,
.nav-box.type6 .prev{
  background: url('data:image/png;base64,') no-repeat left 26rpx center;
  background-size: 21rpx 33rpx;
}


.city {
  position: absolute;
  top: 0;
  left: 20rpx;
  color: #fff;
  font-size: 30rpx;
  z-index: 7;
  height: 45px;
  display: flex;
  align-items: center;
}

.city icon {
  margin-right: 6rpx;
}

.nav-box.type6 .nav-bar {
  background-color: #2fbe82;
}

/* 个人中心 */
.nav-box.type1,
.nav-box.type1 .nav-bar {
  background: #2fbe82;
}

.nav-box.type2,
.nav-box.type2 .nav-bar {
  background-color: #fff;
}


/* 没得border-bottom */
.nav-box.type10 {

}

.nav-info-box {
  height: 45px;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
}
.nav-info-box .headimg {
  width: 46rpx;
  height: 46rpx;
  border-radius: 100%;
}

.nav-info-box .name {
  color: #ffffff;
  font-size: 34rpx;
  margin-left: 16rpx;
}



