.dialog-tips-dec {
  text-align: left;
  padding: 30rpx 0;
  color: #333333;
}

.report-content {
  padding: 0 20rpx;
  margin-top: 30rpx;
}

.report-content-dec {
  margin-top: 40rpx;
  font-size: 24rpx;
  border: 4rpx dashed #f69b18;
